{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "graphql",
    "html",
    "yaml",
    "json",
    "python"
  ],

  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "cSpell.allowCompoundWords": true,
  "prettier.singleQuote": false,
  "cSpell.diagnosticLevel": "Hint"
}
